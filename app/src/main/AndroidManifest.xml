<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.president_of_usa">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.activity_displayInfo" android:parentActivityName=".Activity.activity_InputInfo"></activity>
        <activity android:name=".Activity.activity_InputInfo" android:parentActivityName=".Activity.activity_dashboard"/>
        <activity android:name=".Activity.activity_dashboard">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.activity_details"
            android:parentActivityName="com.example.president_of_usa.Activity.MainActivity" />
        <activity android:name=".Activity.MainActivity" android:parentActivityName=".Activity.activity_dashboard"/>
    </application>

</manifest>